					<jaxb:baseType>
						<jaxb:javaType
							name="eu.europa.esig.jaxb.trustedlist.ecc.enums.KeyUsageBit"
							parseMethod="eu.europa.esig.jaxb.trustedlist.ecc.parsers.KeyUsageBitParser.parse"
