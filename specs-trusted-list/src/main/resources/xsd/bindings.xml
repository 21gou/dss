	<jaxb:globalBindings>
		<jaxb:serializable uid="1" />
		
	  	<jaxb:javaType name="java.util.Date" xmlType="xs:dateTime"
	          parseMethod="eu.europa.esig.dss.jaxb.parsers.DateParser.parse"
	          printMethod="eu.europa.esig.dss.jaxb.parsers.DateParser.print" />
	</jaxb:globalBindings>

	<jaxb:bindings schemaLocation="ts_119612v020101_sie_xsd.xsd" node="/xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="eu.europa.esig.jaxb.trustedlist.ecc" />
		</jaxb:schemaBindings>
		
		<jaxb:bindings node="//xs:complexType[@name='KeyUsageBitType']/xs:simpleContent/xs:extension">
			<jaxb:bindings node="xs:attribute[@name='name']">
